{
  "manifest_version": 3,
  "name": "公文自動存查-(20250320)",
  "version": "1.1",
  "description": "臺北市公文自動存查，支援多入口網域",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://edoc.gov.taipei/*",
        "https://doc.gov.taipei/*" 
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
